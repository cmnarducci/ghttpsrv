         "name": "Launch with Debug",
         "type": "cppdbg",
         "request": "launch",
         "program": "${workspaceFolder}/ghttpsrv.exe",
         "args": [],
         "stopAtEntry": false,
         "cwd": "${workspaceFolder}",
         "environment": [],
         "externalConsole": true,
         "MIMode": "gdb",
         "miDebuggerPath": "C:\\cygwin64\\bin\\gdb.exe",
         "setupCommands": [
             {
                 "description": "Enable pretty-printing for gdb",
